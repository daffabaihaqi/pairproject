<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pamungkas Sports Club</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
    <%- include('../partials/navbar-admin.ejs') %>

    <div class="album py-5">
        <div class="container">
            <div class="row g-3">
                <% courts.forEach((el) => { %>
                <div class="col-xl-3 col-lg-6 col-sm-6">
                    <div class="card shadow-sm h-100" style="border-radius: 5%;">
                        <img class="card-img-top h-60" src="<%= el.imageURL  %>" alt="">
                        <div class="card-body">
                            <!-- <h3 class="card-title text-center" style="height: 20px;"><%= el.name %></h3> -->
                            <p class="font-weight-bold"><%= el.name %></p>
                            <p class="card-text"><%= el.location %></p>
                            <div class="d-flex justify-content-between align-items-center">
                                <div class="btn-group">
                                    <a class="btn btn-primary" href="/admin/detail/<%= el.id %>">See Details</a>
                                    <a class="btn btn-danger" href="/admin/update/<%= el.id %>">Update</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
    </div>
</div>
    
</body>
</html>